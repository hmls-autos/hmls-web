{
  "name": "@hmls/diagnostic-agent",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "deploy": {
    "org": "spinsirr",
    "app": "hmls-diagnostic-agent"
  },
  "tasks": {
    "dev": "deno run --env=../../.env.local --allow-all --watch src/main.ts",
    "start": "deno run --env=../../.env.local --allow-all src/main.ts",
    "check": "deno check src/main.ts",
    "test": "deno test --allow-all src/test/"
  },
  "imports": {
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2.95.0",
    "@react-pdf/renderer": "npm:@react-pdf/renderer@^4.3.2",
    "react": "npm:react@^19.2.3",
    "@std/assert": "jsr:@std/assert@^1.0.0"
  }
}
